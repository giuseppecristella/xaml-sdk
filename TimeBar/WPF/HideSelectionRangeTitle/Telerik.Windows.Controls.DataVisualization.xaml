    <Setter Property="BorderThickness" Value="{StaticResource Timeline_TimelineItemGroup_ToggleButton_OuterBorder_Thickness}" />
    <Setter Property="BorderBrush" Value="{StaticResource Timeline_TimelineItemGroup_ToggleButton_OuterBorder_BorderBrush}" />
    <Setter Property="Background" Value="{StaticResource Timeline_TimelineItemGroup_ToggleButton_OuterBorder_Background}" />
    <Setter Property="CornerRadius" Value="{StaticResource Timeline_TimelineItemGroup_ToggleButton_OuterBorder_CornerRadius}" />
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="telerik:RadToggleButton">
          <Grid Background="#01FFFFFF">
            <VisualStateManager.VisualStateGroups>
              <VisualStateGroup x:Name="CommonStates">
                <VisualState x:Name="Normal" />
                <VisualState x:Name="MouseOver">
                  <Storyboard>
                    <ObjectAnimationUsingKeyFrames Storyboard.TargetName="buttonBorder" Storyboard.TargetProperty="BorderBrush" Duration="0.00:00:00.05">
                      <DiscreteObjectKeyFrame KeyTime="0.00:00:00.0" Value="{StaticResource Timeline_TimelineItemGroup_ToggleButton_OuterBorder_BorderBrush_MouseOver}" />
                    </ObjectAnimationUsingKeyFrames>
                    <ObjectAnimationUsingKeyFrames Storyboard.TargetName="buttonBorder" Storyboard.TargetProperty="Background" Duration="0.00:00:00.05">
                      <DiscreteObjectKeyFrame KeyTime="0.00:00:00.0" Value="{StaticResource Timeline_TimelineItemGroup_ToggleButton_OuterBorder_Background_MouseOver}" />
                    </ObjectAnimationUsingKeyFrames>
                    <ObjectAnimationUsingKeyFrames Storyboard.TargetName="expandButton" Storyboard.TargetProperty="Fill" Duration="0.00:00:00.05">
                      <DiscreteObjectKeyFrame KeyTime="0.00:00:00.0" Value="{StaticResource Timeline_TimelineItemGroup_ToggleButton_Button_Fill_MouseOver}" />
                    </ObjectAnimationUsingKeyFrames>
                    <ObjectAnimationUsingKeyFrames Storyboard.TargetName="collapseButton" Storyboard.TargetProperty="Fill" Duration="0.00:00:00.05">
                      <DiscreteObjectKeyFrame KeyTime="0.00:00:00.0" Value="{StaticResource Timeline_TimelineItemGroup_ToggleButton_Button_Fill_MouseOver}" />
                    </ObjectAnimationUsingKeyFrames>
                  </Storyboard>
                </VisualState>
                <VisualState x:Name="Disabled">
                  <Storyboard>
                    <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Visibility)" Storyboard.TargetName="buttonBorder">
                      <DiscreteObjectKeyFrame KeyTime="0">
                        <DiscreteObjectKeyFrame.Value>
                          <Visibility>Collapsed</Visibility>
                        </DiscreteObjectKeyFrame.Value>
                      </DiscreteObjectKeyFrame>
                    </ObjectAnimationUsingKeyFrames>
                  </Storyboard>
                </VisualState>
                <VisualState x:Name="DisabledChecked">
                  <Storyboard>
                    <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Visibility)" Storyboard.TargetName="buttonBorder">
                      <DiscreteObjectKeyFrame KeyTime="0">
                        <DiscreteObjectKeyFrame.Value>
                          <Visibility>Collapsed</Visibility>
                        </DiscreteObjectKeyFrame.Value>
                      </DiscreteObjectKeyFrame>
                    </ObjectAnimationUsingKeyFrames>
                  </Storyboard>
                </VisualState>
              </VisualStateGroup>
              <VisualStateGroup x:Name="CheckStates">
                <VisualState x:Name="Checked">
                  <Storyboard>
                    <DoubleAnimation Duration="0" Storyboard.TargetName="collapseButton" Storyboard.TargetProperty="Opacity" To="1" />
                    <DoubleAnimation Duration="0" Storyboard.TargetName="expandButton" Storyboard.TargetProperty="Opacity" To="0" />
                  </Storyboard>
                </VisualState>
                <VisualState x:Name="Unchecked" />
              </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <StackPanel Orientation="Horizontal">
              <Border x:Name="buttonBorder" Width="{StaticResource Timeline_TimelineItemGroup_ToggleButton_OuterBorder_Width}" Height="{StaticResource Timeline_TimelineItemGroup_ToggleButton_OuterBorder_Height}" Margin="3,0,0,0" BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}" Background="{TemplateBinding Background}" CornerRadius="{TemplateBinding CornerRadius}" HorizontalAlignment="Left" VerticalAlignment="Center">
                <Grid>
                  <Path x:Name="expandButton" Width="{StaticResource Timeline_TimelineItemGroup_ToggleButton_Button_Width}" Height="{StaticResource Timeline_TimelineItemGroup_ToggleButton_ExpandButton_Height}" Fill="{StaticResource Timeline_TimelineItemGroup_ToggleButton_Button_Fill}" Data="M3,0 L5,0 5,3 8,3 8,5 5,5 5,8 3,8 3,5 0,5 0,3 3,3 z" Opacity="1" />
                  <Rectangle x:Name="collapseButton" Width="{StaticResource Timeline_TimelineItemGroup_ToggleButton_Button_Width}" Height="{StaticResource Timeline_TimelineItemGroup_ToggleButton_CollapseButton_Height}" VerticalAlignment="Center" HorizontalAlignment="Center" Fill="{StaticResource Timeline_TimelineItemGroup_ToggleButton_Button_Fill}" Opacity="0" />
                </Grid>
              </Border>
              <TextBlock Text="{TemplateBinding Content}" Margin="4 0 0 0" />
            </StackPanel>
          </Grid>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
